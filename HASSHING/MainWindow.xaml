<Window x:Class="HASSHING.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HASSHING"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="HashedText" Content="Label" HorizontalAlignment="Left" Margin="490,100,0,0" VerticalAlignment="Top" Width="275" Height="79"/>
        <TextBox x:Name="Message" Margin="490,23,35,0" TextWrapping="Wrap" Text="Message" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.726"/>
        <TextBox x:Name="Key" Margin="490,46,35,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.726"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="10,158,0,0" VerticalAlignment="Top" Width="130" Click="Button_Click"/>
        <ComboBox x:Name="Select" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="200" Height="31" SelectionChanged="ComboBox_SelectionChanged">
            <ComboBoxItem IsSelected="True">MD5</ComboBoxItem>
            <ComboBoxItem IsSelected="False">SHA1</ComboBoxItem>
            <ComboBoxItem IsSelected="False">SHA256</ComboBoxItem>
        </ComboBox>

    </Grid>
</Window>
